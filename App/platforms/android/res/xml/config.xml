<?xml version='1.0' encoding='utf-8'?>
<widget id="myFields.Reporting" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.Geolocation" />
    </feature>
    <name>myFieldsReportingApp</name>
    <description>
        An Apache Cordova application for Android and Web that allows farmers to report issues with their crops to the KSU Extension office.
    </description>
    <author email="nhbean@ksu.edu" href="cs.ksu.edu">
        2016-2017 KSU Senior Software Engineering Project Team under Nathan Bean
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
